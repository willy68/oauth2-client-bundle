{
    "name": "willy68/psr-oauth2-client",
    "type": "psr-oauth2-client",
    "description": "Integration with league/oauth2-client to provide services",
    "keywords": ["oauth", "oauth2"],
    "homepage": "https://symfonycasts.com",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Ryan Weaver",
            "email": "ryan@symfonycasts.com"
        },
        {
            "name": "William Lety",
            "email": "william.lety@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "psr/container": "^2.0",
        "psr/http-message": "^1.0",
        "psr/http-factory": "^1.0",
        "league/oauth2-client": "^2.0"
    },
    "autoload": {
        "psr-4": { "KnpU\\OAuth2ClientBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "KnpU\\OAuth2ClientBundle\\Tests\\": "tests/" }
    },
    "require-dev": {
        "league/oauth2-facebook": "^1.1|^2.0",
        "php-di/php-di": "^7.0",
        "mezzio/mezzio-fastroute": "3.8.0",
        "mezzio/mezzio-session": "1.12.0",
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.10.1"
    },
    "scripts": {
        "test": "phpunit && phpstan analyze"
    }
}
